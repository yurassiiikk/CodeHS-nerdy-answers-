public class TwoTowersKarel extends SuperKarel
{

	public void run()
	{
		move();
		towerOne();
		goBack();
		move();
		move();
		towerOne();
		
	}
	private void goBack()
	{
	    turnRight();
	    move();
	    move();
	    move();
	    turnLeft();
	}
	
	private void towerOne ()
	{
	    putBall();
	    turnLeft();
	    move();
	    putBall();
	    move();
	    putBall();
	    move();
	    turnRight();
	}
	
}
