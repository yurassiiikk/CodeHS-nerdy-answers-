public class Circle
{
    private double radius;
 
    public Circle(double theRadius)
    {
        radius = theRadius;
    }
    public double getArea()
    {
      Double Area = Math.PI * Math.pow(radius, 2);
      return Area;  
    }
    public double getCircumference()
    {
    double Circumference = 2 * Math.PI * radius;
       return Circumference;
    }

}
